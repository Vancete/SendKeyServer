<Window x:Class="SendKeyServer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SendKeyServer"
        mc:Ignorable="d"
        Title="SendKeyServer" Height="350" Width="525" Icon="res/sks.ico">
    <Grid Background="#FFE2E2E2">
        <ListBox x:Name="listBox" Margin="0,54,0,0"/>
        <Image x:Name="image" HorizontalAlignment="Left" Height="44" VerticalAlignment="Top" Width="44" Source="res/sks.png" Margin="7,5,0,0"/>
        <Label x:Name="name" Content="SendKeyServer" HorizontalAlignment="Left" Margin="57,-1,0,0" VerticalAlignment="Top" FontSize="24"/>
        <Label x:Name="version" Content="v1.0" HorizontalAlignment="Left" Margin="218,14,0,0" VerticalAlignment="Top" FontSize="10"/>
        <Label x:Name="status" Content="Starting..." HorizontalAlignment="Left" Margin="57,28,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
